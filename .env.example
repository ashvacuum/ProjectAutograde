# Unity Auto-Grader Environment Variables
# Copy this file to .env and fill in your actual values

# Canvas LMS Configuration
CANVAS_API_URL=https://your-school.instructure.com/api/v1
CANVAS_TOKEN=your_canvas_api_token_here
# Alternative: CANVAS_API_KEY=your_canvas_api_token_here (both work)

# GitHub Access (Optional - for private repositories)
GITHUB_TOKEN=your_github_personal_access_token_here

# Application Settings
NODE_ENV=production
LOG_LEVEL=info

# Temporary Directory (Optional - defaults to system temp)
TEMP_DIR=

# Export Directory (Optional - defaults to app directory)
EXPORT_DIR=